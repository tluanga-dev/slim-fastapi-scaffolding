{
  "id": null,
  "title": "Rental Management System - Performance",
  "tags": [
    "rental",
    "performance",
    "monitoring"
  ],
  "timezone": "browser",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Request Throughput",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(rental_management_requests_total[5m])",
          "refId": "A",
          "legendFormat": "{{method}} {{endpoint}}"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "Requests/sec",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Response Time Distribution",
      "type": "heatmap",
      "targets": [
        {
          "expr": "rate(rental_management_request_duration_seconds_bucket[5m])",
          "refId": "A",
          "legendFormat": "{{le}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Database Query Performance",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(rental_management_db_query_duration_seconds_bucket[5m]))",
          "refId": "A",
          "legendFormat": "95th percentile - {{operation}} {{table}}"
        },
        {
          "expr": "histogram_quantile(0.99, rate(rental_management_db_query_duration_seconds_bucket[5m]))",
          "refId": "B",
          "legendFormat": "99th percentile - {{operation}} {{table}}"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "Query Duration (seconds)",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Database Query Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(rental_management_db_queries_total[5m])",
          "refId": "A",
          "legendFormat": "{{operation}} {{table}}"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "Queries/sec",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 5,
      "title": "Memory Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "rental_management_memory_usage_bytes",
          "refId": "A",
          "legendFormat": "{{type}}"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "Bytes",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 6,
      "title": "CPU Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "rental_management_cpu_usage_percent",
          "refId": "A",
          "legendFormat": "CPU Usage"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "Percent",
          "show": true,
          "min": 0,
          "max": 100
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 16
      }
    },
    {
      "id": 7,
      "title": "Disk Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "rental_management_disk_usage_bytes",
          "refId": "A",
          "legendFormat": "{{path}}"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "Bytes",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 16
      }
    },
    {
      "id": 8,
      "title": "Cache Performance",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(rental_management_cache_hits_total[5m])",
          "refId": "A",
          "legendFormat": "Cache Hits - {{cache_type}}"
        },
        {
          "expr": "rate(rental_management_cache_misses_total[5m])",
          "refId": "B",
          "legendFormat": "Cache Misses - {{cache_type}}"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "Operations/sec",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 9,
      "title": "Error Breakdown",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(rental_management_errors_total[5m])",
          "refId": "A",
          "legendFormat": "{{error_type}} - {{module}}"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "Errors/sec",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      }
    }
  ]
}
